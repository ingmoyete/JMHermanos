                <a href="@Resources.Routes.urlTrabajos/@Resources.Routes.urlFachadas" class="link blackFont bold">VER MAS&nbsp;&nbsp;<span class="fa fa-arrow-circle-right"></span></a>
            </div>

            <div class="secondCol yellow text-center centerInsideV">
                <span>F</span>
            </div>

